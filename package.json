  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "deploy:contracts": "forge script script/Deploy.s.sol --rpc-url $BASE_RPC_URL --broadcast",
    "deploy:setup-keystore": "./scripts/setup-keystore.sh",
    "deploy:production": "./scripts/deploy-production.sh",
    "deploy:production:dry-run": "./scripts/deploy-production.sh --dry-run",
    "deploy:production:compute": "./scripts/deploy-production.sh --compute-only",
    "deploy:production:verify": "./scripts/deploy-production.sh --verify",
    "migrate:production": "./scripts/migrate-with-keystore.sh",
    "dev:local": "./scripts/start-local-dev.sh",
    "dev:local:migrate": "./scripts/start-local-dev.sh --migrate",
    "dev:mai": "./scripts/with-mai-api.sh",
    "develop": "vite",
    "start": "vite",
    "serve": "vite preview",
    "dev:help": "echo -e '\\nðŸ“š Local Development Commands:\\n\\n  ðŸš€ Start Services:\\n    bun run dev:local                                      - Start full local environment\\n    bun run dev:local:migrate                              - Start with QIP migration (209-248)\\n    bun run dev:mai                                        - Start with Mai API backend\\n\\n  ðŸ”® Deterministic Addresses:\\n    bun run dev:compute-address                            - Show addresses for test accounts\\n    bun run dev:compute-address -- --impersonate=<addr>    - Compute address for YOUR wallet\\n    bun run dev:impersonate -- --impersonate=<addr>        - Deploy locally as specific address\\n\\n  ðŸ“¦ Migration & Setup:\\n    bun run migrate:original-numbers-dev                   - Migrate QIPs 209-248\\n    bun run ipfs:setup                                     - Configure local IPFS\\n\\n  ðŸ’¡ Examples:\\n    bun run dev:compute-address -- --impersonate=0x742d35Cc6634C0532925a3b844Bc9e7595f0bEb7\\n    bun run dev:local -- --impersonate=0xYourAddress --migrate\\n'",
    "dev:anvil": "./scripts/startAnvil.sh",
    "dev:test-client": "bun run src/localQIPTest.ts",
    "dev:deploy-only": "forge script script/DeployOnly.s.sol:DeployOnly --rpc-url http://localhost:8545 --broadcast",
    "ipfs:setup": "./scripts/setup-ipfs.sh",
    "ipfs:check": "./scripts/check-ipfs.sh",
    "ipfs:seed": "bun run ./scripts/seed-ipfs-test-data.ts",
    "predev:local": "bun run ipfs:check",
    "dev:with-ipfs": "USE_LOCAL_IPFS=true bun run dev"
  },
