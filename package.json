{
  "name": "qips",
  "version": "1.0.0",
  "private": true,
  "description": "qips",
  "author": "Tekrajs & Publu",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "deploy:contracts": "forge script script/Deploy.s.sol --rpc-url $BASE_RPC_URL --broadcast",
    "migrate:qips": "bun scripts/migrate-qips.ts",
    "migrate:local-ipfs-dev": "bun run scripts/migrate-to-local-ipfs-dev.ts",
    "migrate:original-numbers-dev": "bun run scripts/migrate-with-original-numbers-dev.ts",
    "migrate:local-ipfs:dry": "bun run scripts/migrate-to-local-ipfs.ts --dry-run",
    "dev:local": "./scripts/start-local-dev.sh",
    "develop": "vite",
    "start": "vite",
    "serve": "vite preview",
    "dev:help": "echo 'Local Development Commands:\\n  npm run dev:local - Start full local environment (IPFS + Anvil + Vite)\\n  npm run migrate:local-ipfs-dev - Upload QIPs to local IPFS (dev only)\\n  npm run migrate:original-numbers-dev - Migrate QIPs 209-248 with original numbers (dev only)'",
    "dev:anvil": "./scripts/startAnvil.sh",
    "dev:test-client": "bun run src/localQIPTest.ts",
    "dev:deploy-only": "forge script script/DeployOnly.s.sol:DeployOnly --rpc-url http://localhost:8545 --broadcast",
    "ipfs:setup": "./scripts/setup-ipfs.sh",
    "ipfs:check": "./scripts/check-ipfs.sh",
    "ipfs:seed": "bun run ./scripts/seed-ipfs-test-data.ts",
    "predev:local": "bun run ipfs:check",
    "dev:with-ipfs": "USE_LOCAL_IPFS=true bun run dev"
  },
  "dependencies": {
    "@mdx-js/react": "^2.3.0",
    "@snapshot-labs/snapshot.js": "^0.13.0",
    "@tanstack/react-query": "^5.80.6",
    "autoprefixer": "^10.4.16",
    "connectkit": "^1.9.0",
    "ethers": "^5.8.0",
    "gatsby": "^5.13.0",
    "gatsby-plugin-image": "^3.12.1",
    "gatsby-plugin-manifest": "^5.12.1",
    "gatsby-plugin-mdx": "^5.12.1",
    "gatsby-plugin-postcss": "^6.12.0",
    "gatsby-plugin-sharp": "^5.12.1",
    "gatsby-plugin-sitemap": "^6.12.1",
    "gatsby-source-filesystem": "^5.12.0",
    "gatsby-transformer-remark": "^6.12.1",
    "gatsby-transformer-sharp": "^5.12.1",
    "graphql-request": "^6.1.0",
    "lodash": "^4.17.21",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "swr": "^2.2.4",
    "tailwindcss": "^3.3.3",
    "viem": "2.x",
    "wagmi": "^2.15.5"
  },
  "overrides": {
    "@parcel/watcher": "2.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.6.0",
    "dotenv": "^17.2.0",
    "react-router-dom": "^7.7.0",
    "typescript": "^5.1.6",
    "vite": "^7.0.4"
  }
}
